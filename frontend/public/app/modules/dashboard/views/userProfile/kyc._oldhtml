                                     <!--   <select class="custom-select" ng-model="vm.idType.issuing_authority" name="authority" required>
                                         <option style="display:none" value="">Select Issuing Authority</option>
                                         <option value="{{item.name}}" ng-repeat="item in vm.getIssueAuth" ng-disabled="!item.status">{{item.name}}</option>
                                     </select> -->
                                     <select class="custom-select form-control" ng-model="vm.idType.issuing_country" ng-options="item.name for item in vm.getCountry track by item.id" required autocomplete="off" name="authority" ng-change="vm.changeCountry(vm.idType.issuing_country)">
                                         <option style="display:none" value="">Select Issuing Country</option>
                                     </select>
                                     <p class="error-msg" ng-if="vm.idForm.authority.$error.required && vm.idForm.$submitted">Please select Issuing Country.</p>
                                 </div>
                             <div class="col-sm-6 form-group">
                                     <label class="form-group-label">ID Type<sup class="important">*</sup></label>

                                     <select class="custom-select form-control" ng-model=" vm.idType.doc_name" name="idtype" required ng-options="item.name for item in vm.getIDType track by item.id" autocomplete="off" id="docName" ng-change="vm.changeIdType(vm.idType.doc_name);">
                                         <option style="display:none" value="">Select ID Type</option>
                                     </select>
                                     <p class="error-msg" ng-if="vm.idForm.idtype.$error.required && vm.idForm.$submitted">Please select ID Type.</p>

                             </div>

                 <div class="form-group col-sm-6">
                         <label class="form-group-label">{{vm.idType.doc_name ? vm.idType.doc_name.name : 'Identity Card'}} Number<sup class="important">*</sup></label>
                         <input class="form-control" type="text" ng-model="vm.idType.doc_reference" required pattern="^[a-zA-Z0-9]*$" maxlength="20" minlength="3" autocomplete="off" name="idnum">
                         <p class="error-msg" ng-if="vm.idForm.idnum.$error.required && vm.idForm.$submitted">Please enter Identity Number.</p>
                         <p class="error-msg" ng-if="vm.idForm.idnum.$error.pattern && vm.idForm.idnum.$touched">Please enter valid Identity Number.</p>
                 </div>

                 <div class="form-group col-sm-6">
                     <div class="has-form-group-icon relative-element">
                             <label class="form-group-label">{{vm.idType.doc_name ? vm.idType.doc_name.name : 'Identity Card'}} Issue Date<sup class="important">*</sup></label>
                                 <input type="text" class="form-control" uib-datepicker-popup="{{$root.format}}" ng-model="vm.idType.issue_date" is-open="vm.issueDate.opened" datepicker-options="vm.dateOptions" close-text="Close" autocomplete="off" required="true" name="issueDate" onkeypress="return false;" />
                                   <span class="form-group-icon fa fa-calendar calendar-icon" ng-click="vm.issueDate.opened = true;"></span>
                                   </div>
                             <p class="error-msg" ng-if="vm.idForm.issueDate.$error.required && vm.idForm.$submitted">Please select Issue Date.</p>
                 </div>

                 <div class="form-group col-sm-6">
                     <div class="has-form-group-icon relative-element">
                         <label class="form-group-label">{{vm.idType.doc_name ? vm.idType.doc_name.name : 'Identity Card'}} Expiration Date</label>
                             <input type="text" class="form-control" uib-datepicker-popup="{{$root.format}}" ng-model="vm.idType.expiration_date" is-open="vm.expDate.opened" datepicker-options="vm.dateExpiryOptions" close-text="Close" autocomplete="off" readonly="true"/>
                             <span class="form-group-icon fa fa-calendar calendar-icon" ng-click="vm.expDate.opened = true;"></span>
                             </div>
                 </div>

                 <div class="col-sm-6 form-group">
                         <label class="form-group-label">Upload your ID - Passport or Singapore NRIC front<small style="font-size: 80% !important">&nbsp;&nbsp;(Maximum file upload size : 1MB. Allowed Formats : JPG,PNG,PDF,DOC,DOCX )</small></label>
                         <!-- <label class="form-group-label">Upload ID Proof - Front<sup class="important" ng-if="!vm.idProofUploaded">*</label>
                         <div class="row"><div class="col-12"><small class="text-danger" style="font-size: 80% !important">&nbsp;&nbsp;(Maximum file upload size : 1MB. Allowed Formats : JPG,PNG,PDF,DOC,DOCX)</small></div></div> -->
                         <!-- <p class="marT5 thin-font">*.png .jpg and jpeg format supported. File size should be less than 5 MB</p> -->

                         <div class="file-input-box">
                             <div class="row">

                                 <div class="col-md-8 pull-md-4 file-input-text">
                                   <div class="custom-input-file">
                                       <input type="file" class="inputfile" ngf-select ng-model="vm.file" name="file" accept=".doc,.docx,.pdf,.jpeg,.jpg,.png" ngf-pattern="'.doc,.docx,.pdf,.jpeg,.jpg,.png'" ngf-max-size="1MB" ng-required="!vm.idProofUploaded" style="padding:0px;" />
                                       <a href="javascript:;" class="btn btn-outline-primary">Upload Image</a>
                                       <div class="selected-value marT15"><span class="text"></span><span class="input-close">×</span></div>
                                   </div>
                                     <p class="mt-2 error-msg" ng-if="vm.idForm.file.$error.required && vm.idForm.$submitted">Please upload ID Proof.</p>
                                     
                                     <p class="mt-2 error-msg" ng-if="vm.idForm.file.$error.pattern && vm.idForm.file.$touched">Please upload a valid
                                     (.jpg, .png, .doc, .docx, .pdf) file only.</p>
                                     <p class="mt-2 error-msg" ng-if="vm.idForm.file.$error.maxSize">Maximum file upload size : 1MB.</p>
                                 </div>

                             </div>

                     </div>

                     </div>
                     <div class="form-group col-sm-6"><!--  uploaded-photo-wrap -->

                      <label class="form-group-label">Uploaded ID Proof - Passport or Singapore NRIC front</label>
                         <span ng-if="vm.idProofUploaded">
                           <a href="" ng-click="vm.downloadDoc(vm.idProofFront_uploaded_href)">
                             <img style="width:100px;" ngf-thumbnail="vm.idProofUploaded || '/thumb.jpg'"/>
                           </a>
                         </span>
                         <span ng-if="!vm.idProofUploaded"><i>No ID Proof Uploaded</i></span>
                     </div>


