                 <!-- kyc address tab -->
                 <div class="tab-pane kycAddress">
                     <form class="form" name="vm.poaForm" novalidate>
                   <!-- Part -->
                   <div class="row">
                     <div class="col-md-6 form marT20">
                    <h5>Residential Address</h5>

                    <div class="form-group marT30" style="margin-top: 32px;">
                         <label class="form-group-label">Address<sup class="important">*</sup>
                         </label>
                         <input type="hidden" ng-model="vm.poaRes.doc_type" ng-init="vm.poaRes.doc_type = 5">
                         <textarea class="form-control" ng-model="vm.poaRes.res_address" required autocomplete="off" name="street" maxlength="400" rows="4" cols="50" style=" resize: none;" /></textarea>
                         <p class="error-msg" ng-if="vm.poaForm.street.$error.required && vm.poaForm.$submitted">Please enter Address.</p>
                     </div>

                     <div class="form-group">
                             <label class="form-group-label">ZIP Code<sup class="important">*</sup></label>
                              <input type="text" class="form-control" ng-model="vm.poaRes.res_pin_code" required pattern="^[- a-zA-Z0-9]*$" maxlength="10" autocomplete="off" name="zip" />
                              <p class="error-msg" ng-if="vm.poaForm.zip.$error.required && vm.poaForm.$submitted">Please enter ZIP Code.</p>
                              <p class="error-msg" ng-if="vm.poaForm.zip.$error.pattern && vm.poaForm.zip.$touched">Please enter valid ZIP Code.</p>
                     </div>
                     <div class="form-group">
                             <label class="form-group-label">Country<sup class="important">*</sup></label>
                             <select class="custom-select form-control" ng-model="vm.poaRes.res_country" ng-options="item.name for item in vm.getCountry track by item.id" ng-change="vm.getStatesByCountryId_Res(vm.poaRes.res_country)" required autocomplete="off" name="rescountry">
                                 <option style="display:none" value="">Select Country</option>
                             </select>
                             <p class="error-msg" ng-if="vm.poaForm.rescountry.$error.required && vm.poaForm.$submitted">Please select Country.</p>
                     </div>
                     <div class="form-group">
                             <label class="form-group-label">State/Province<sup class="important">*</sup></label>
                             <select class="custom-select form-control" ng-model="vm.poaRes.res_state" ng-options="item.name for item in vm.getStatesRes track by item.id" required ng-disabled="!vm.poaRes.res_country" ng-change="vm.getCitiesByStateId_Res();" name="state">
                                 <option style="display:none" value="">Select State/Province</option>
                             </select>
                             <p class="error-msg" ng-if="vm.poaForm.state.$error.required && vm.poaForm.$submitted && vm.poaRes.res_country">Please select State/Province.</p>
                             <p ng-if="vm.getStatesRes.length == 0 && vm.poaRes.res_country">No states available for the selected country.</p>
                     </div>
                     <div class="form-group">
                             <label class="form-group-label">City<sup class="important">*</sup></label>
                             <select class="custom-select form-control" ng-model="vm.poaRes.res_city" ng-options="item.name for item in vm.getCitiesRes track by item.id" required autocomplete="off" ng-disabled="!vm.poaRes.res_state" name="city">
                                 <option style="display:none" value="">Select City</option>
                             </select>
                             <p class="error-msg" ng-if="vm.poaForm.city.$error.required && vm.poaForm.$submitted && vm.poaRes.res_state">Please select City.</p>
                             <p ng-if="vm.getCitiesRes.length == 0 && vm.poaRes.res_state">No cities available for the selected state.</p>
                     </div>
                     <div class="form-group">
                             <label class="form-group-label">Address Proof Type<sup class="important">*</sup></label>
                             <!--  <select class="custom-select form-control" ng-model="vm.poaRes.doc_name" required name="proof">
                                 <option style="display:none" value="">Select Address Proof Type</option>
                                 <option value="{{item.name}}" ng-repeat="item in vm.getAddrProofTypes" ng-disabled="!item.status">{{item.name}}</option>
                             </select> -->
                             <select class="custom-select form-control" ng-model="vm.poaRes.doc_name" required name="proof" ng-options="item.name for item in vm.getAddrProofTypes track by item.id">
